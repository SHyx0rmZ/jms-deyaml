{
    "name": "ppokatilo/jms-deyaml",
    "type": "library",
    "description": "Visitor for deserializing YAML with JMS serializer",
    "keywords": [ "deserialization", "yaml" , "jms" ],
    "homepage": "http://github.com/SHyx0rmZ/jms-deyaml",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Pokatilo",
            "email": "mail@shyxormz.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "jms/serializer": "~0.11",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "SHyx0rmZ\\JMSDeYaml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SHyx0rmZ\\JMSDeYaml\\Test\\": "tests/"
        }
    }
}
